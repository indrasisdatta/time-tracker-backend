{
  "version": 2,
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "dist/src/server.js"
    }
  ]
}
